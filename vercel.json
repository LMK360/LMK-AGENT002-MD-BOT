{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "SESSION_ID": "your_session_id",
    "BOT_NAME": "MyBot",
    "OWNER_NAME": "YourName",
    "API_KEY": "your_api_key"
  }
}
